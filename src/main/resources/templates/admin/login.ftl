  <body>
    <div class="lg_hd">
      <!-- 仅首页需要添加ctriplogoindex这个class -->
      <h1 class="ctriplogo_login ctriplogoindex">
        <a href="javascript:void(0);" title="忠华棉花管理系统">忠华棉仓管理系统</a></h1>
    </div>
    <div class="lg_bd" id="lg_bd">
      <!-- 广告图片区域 -->
      <div class="lg_banner_wrap">
        <div class="lg_banner_list jmkt-picsroller" id="bannerpic">
          <div class="jmkt-picsroller-pics" style="z-index: 1;">
            <a href="javascript:void(0);">
              <img src="/img/dlbj.png" width="1920" height="540"></a>
          </div>
        </div>
      </div>
      <div class="mod-box" style="z-index: 10;">
      	<form id="login_from" action="/signin" method="post">
      		<input type="hidden" id="target" name="target" value="${target!''}"/>
      		<input type="hidden" id="slidetounlock" value="0"/>
	        <div class="lg_loginwrap" id="loginwrap">
	          <div class="lg_loginbox" id="lg_loginbox" style="top: 10px; margin-top: 30px;">
	            <h2 class="lg_loginbox_title" id="logintitle">账号登录</h2>
	            <div class="form_wrap" id="normalview" style="display: block;">
	              <dl>
	                <dd>
	                  <input class="r_input" type="text" name="username" id="username" value="${username!}" placeholder="国内手机号/用户名/邮箱"></dd>
	              </dl>
	              <dl>
	                <dd class="r_input input_auth-code">
	                  <input type="password" name="password" id="password" placeholder="登录密码">
	                  <#--<a href="javascript:;" class="forgotpwd" target="_black">忘记密码</a>-->
                    </dd>
	              </dl>
	              <dl>
	                <dd>
	                  	<div class="drag">
					        <div class="bg"></div>
					        <div class="text" onselectstart="return false;">
					        	<span>请按住滑块，拖动到最右边</span>
					        </div>
					        <div class="btn">&gt;&gt;</div>
					    </div>
	                </dd>
	              </dl>
	              <div class="base_error" style="display:none" id="nerr"></div>
	              <dl>
	                <div class="login-set">
	                  <label class="auto-login">
	                    <input type="checkbox" checked="checked" id="normal30day">30天内自动登录</label>
	                  <a href="javascript:;" class="login-entry-dynamic">手机动态密码登录
	                    <i class="icon-arrowr"></i></a>
	                </div>
	                <dd>
	                  <input class="form_btn form_btn--block" id="nsubmit" type="button" value="登   录"></dd>
	              </dl>
	              <p class="agreement-list">登录即代表您同意我们的
	                <a href="javascript:void(0);" class="serviceAgreement" target="_black">服务协议</a>和
	                <a href="javascript:void(0);" class="privacyPolicy" target="_black">隐私政策</a></p>
	            </div>
	            <div class="lg_weblogin" id="loginbanner" style="display: block;">
	              <div class="login-about">
	                <a href="/register" class="free-regist">免费注册</a></div>
	              <div class="login-other">
	                <a href="javascript:;" class="icon-alipay">支付宝</a>
	                <a href="javascript:;" class="icon-qq">QQ</a>
	                <a href="javascript:;" class="icon-baidu">百度</a>
	                <a href="javascript:;" class="icon-sina">新浪微博</a>
	                <a href="javascript:;" class="icon-wechat">微信</a>
	                <a href="javascript:;" style="display:none" class="icon-163">网易</a>
	                <a href="javascript:;" class="icon-renren" style="display: none;">人人网</a></div>
	            </div>
	            <div class="login-code" id="changeScanLogin">
	              <a href="javascript:;" title="扫码登录更安全" id="scanLogin" logintype="scan">扫码登录
	                <i class="icon-ercode"></i></a>
	              <!-- <i class="icon-ercode"></i> --></div>
	            <div class="form_wrap" id="scanloginview" style="display:none">
	              <!-- 单独二维码 -->
	              <div id="qrcode">
	                <div class="er_scan_wrap er_code" id="qrcode_main">
	                  <img id="qrcode_img" alt="">
	                  <img src="" alt="" id="appScan">
	                  <div class="er_void" style="display:none;" id="erCodeExpire">
	                    <p class="void_text" id="qrcodeError">二维码已失效</p>
	                    <a href="javascript:;" class="btn-refresh" id="refresh">刷 新</a></div>
	                </div>
	                <p class="er_text">打开
	                  <a href="javascript:;" target="_blank" id="downloadApp">APP</a>扫描二维码登录</p></div>
	              <div class="er_success" id="scansuccess" style="display:none">
	                <i class="icon-ok-bg"></i>
	                <p class="success_text">扫码成功</p>
	                <p class="success_desc">请在手机上确认登录</p>
	                <p class="success_info">
	                  <a href="javascript:;" id="rescan">重新扫描</a></p>
	              </div>
	            </div>
	         </div>
	      </div>
      </form>
    </div>
    <div class="base_ft_wrap" id="base_ft">
      <p>
        <a href="javascript:void(0);" title="网站导航">网站导航</a>|
        <a href="javascript:void(0);" title="用户协议">用户协议</a>|
        <a href="javascript:void(0);" title="关于忠华">关于忠华</a>|
        <a href="javascript:void(0);" title="企业公民">企业公民</a>|
        <a href="javascript:void(0);" title="诚聘英才">诚聘英才</a>|
        <a href="javascript:void(0);" title="分销联盟">分销联盟</a>|
        <a href="javascript:void(0);" title="产品采购">产品采购</a>|
        <a href="javascript:void(0);" title="代理合作">代理合作</a>|
        <a href="javascript:void(0);" title="广告业务">广告业务</a>|
        <a href="javascript:void(0);" title="联系我们">联系我们</a>|
        <a id="callOld" title="返回旧版" href="">返回旧版</a></p>
      <p class="copyright">
        <a href="javascript:void(0);">Copyright ©</a>
        <span id="copyrightDate">1999-2018</span>,
        <a href="###">gdzhonghua.com</a>. All rights reserved.</p>
    </div>
</body>
    <script type="text/javascript" charset="utf-8" src="/js/login/drag.js"></script>
	<script type="text/javascript">
        var errorMsg = "${errorMsg!""}";
		if(errorMsg!=""){
		    if(errorMsg == "202"){
                errorMsg = "用户名或密码错误！"
            }else if(errorMsg == "203"){
                errorMsg = "用户已被删除，请联系客服！"
            }else if(errorMsg == "203"){
                errorMsg = "用户已被锁定，请联系客服！"
            }
			drag("#nerr").innerHTML = "<i></i>" + errorMsg;
			drag("#nerr").style.display = "block";
		}

		drag("#nsubmit").onclick = function(){
			if(drag("#username").value==""){
				drag("#nerr").innerHTML = "<i></i>请输入账号";
				drag("#nerr").style.display = "block";
			}else if(drag("#password").value==""){
				drag("#nerr").innerHTML = "<i></i>请输入密码";
				drag("#nerr").style.display = "block";
			}else if(drag("#slidetounlock").value=="0"){
				drag("#nerr").innerHTML = "<i></i>请滑动验证码";
				drag("#nerr").style.display = "block";
			}else{
				drag("#login_from").submit();
			}
		};
		
	</script>
</html>